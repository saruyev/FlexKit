{
  "application": {
    "name": "Azure Key Vault Complex Test",
    "version": "2.0.0",
    "environment": "production"
  },
  "database": {
    "primary": {
      "host": "db-primary.example.com",
      "port": 5432,
      "username": "primary_user",
      "password": "primary_password_123",
      "ssl": true,
      "maxConnections": 50,
      "connectionTimeout": 30
    },
    "secondary": {
      "host": "db-secondary.example.com", 
      "port": 5432,
      "username": "secondary_user",
      "password": "secondary_password_456",
      "ssl": true,
      "maxConnections": 25,
      "connectionTimeout": 15
    }
  },
  "cache": {
    "redis": {
      "host": "redis.example.com",
      "port": 6379,
      "password": "redis_password_789",
      "database": 0,
      "ssl": false,
      "keyPrefix": "myapp:",
      "defaultTtl": 3600
    }
  },
  "apis": {
    "external": {
      "paymentService": {
        "baseUrl": "https://payments.example.com/api/v2",
        "apiKey": "payment_api_key_abcdef123456",
        "secret": "payment_secret_ghijkl789012",
        "timeout": 45000,
        "retries": 3
      },
      "notificationService": {
        "baseUrl": "https://notifications.example.com/api/v1",
        "apiKey": "notification_api_key_mnopqr345678",
        "timeout": 30000,
        "retries": 2
      }
    }
  },
  "security": {
    "encryption": {
      "key": "encryption_key_stuvwx901234",
      "algorithm": "AES-256-GCM",
      "keyRotationDays": 90
    },
    "jwt": {
      "secret": "jwt_secret_yzabcd567890",
      "issuer": "myapp.example.com",
      "audience": "myapp-users",
      "expiration": "24h"
    }
  },
  "monitoring": {
    "metrics": {
      "endpoint": "https://metrics.example.com/ingest",
      "apiKey": "metrics_api_key_efghij123456",
      "batchSize": 100,
      "flushInterval": 10
    },
    "logging": {
      "endpoint": "https://logs.example.com/ingest",
      "apiKey": "logging_api_key_klmnop789012",
      "level": "info",
      "structured": true
    }
  }
}