{
  "FlexKit": {
    "Logging": {
      "MaxBatchSize": 1,
      "BatchTimeout": "00:00:01",
      "DefaultFormatter": "StandardStructured",
      "EnableFallbackFormatting": true,
      "AutoIntercept": true,
      "DefaultTarget": "Console",
      "Services": {
        "*.IStandardFormatterService": {
          "LogBoth": true,
          "Level": "Information"
        },
        "*.IJsonFormatterService": {
          "LogBoth": true,
          "Level": "Information"
        },
        "*.IHybridFormatterService": {
          "LogBoth": true,
          "Level": "Information"
        },
        "*.IExactMatchService": {
          "LogBoth": true,
          "Level": "Warning",
          "Target": "Console"
        },
        "*.Wildcard*": {
          "LogInput": true,
          "Level": "Information"
        },
        "*.IExactExclusionService": {
          "LogBoth": true,
          "Level": "Information",
          "ExcludeMethodPatterns": ["ToString", "GetHashCode", "Equals"]
        },
        "*.IPrefixExclusionService": {
          "LogInput": true,
          "Level": "Information",
          "ExcludeMethodPatterns": ["Get*", "Set*", "Validate*"]
        },
        "*.ISuffixExclusionService": {
          "LogOutput": true,
          "Level": "Information",
          "ExcludeMethodPatterns": ["*Internal", "*Helper", "*Cache"]
        },
        "*.IMixedExclusionService": {
          "LogBoth": true,
          "Level": "Information",
          "ExcludeMethodPatterns": ["ToString", "Get*", "*Internal", "*Debug*", "Validate*", "*Cache"]
        }
      },
      "Targets": {
        "Console": {
          "Type": "Console",
          "Enabled": true,
          "Properties": {
            "FormatterType": "Simple",
            "LogLevel": "Debug"
          }
        }
      },
      "Formatters": {
        "StandardStructured": {
          "Enabled": true
        },
        "Json": {
          "PrettyPrint": false,
          "IncludeTimingInfo": true,
          "IncludeThreadInfo": true
        },
        "Hybrid": {
          "IncludeMetadata": true,
          "MetadataSeparator": " | META: "
        }
      }
    }
  }
}